{
  "name": "jhoelzl/magento_vagrant1",
  "minimum-stability":"dev",
  "require":{
    "magento-hackathon/magento-composer-installer": "*",
    "jhoelzl/magento":"dev-master",
    "colinmollenhour/modman":"*",
    "aoepeople/Aoe_Scheduler":"*",
    "yireo/yireo_disablelog":"*",
    "connect20/raveinfosys_deleteorder": "*",
    "aschroder/smtp_pro": "*",
    "connect20/zero1_cookiealert": "*",
    "connect20/aromicon_google_universal_analytics": "*",
    "connect20/shopgo_gtm": "*",
    "firegento/magesetup": "*",
    "gordonlesti/lesti_fpc":"*",
    "avstudnitz/scopehint":"*",
    "zookal/magento-mock":"*",
    "mkleine/magento-language-routes":"dev-master",
    "schumacherfm/markdown":"*",
    "firegento/flexcms":"dev-master"
  },
    
   "repositories":[    
    {
      "type":"composer",
